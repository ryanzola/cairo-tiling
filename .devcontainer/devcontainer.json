{
  "name": "Cairo Tiling",
  "dockerFile": "Dockerfile",
  "appPort": 3000,
  "runArgs": ["-u", "node"],
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.colorTheme": "poimandres",
        "terminal.integrated.shell.lunix": "/bin/bash"
      },
      "extensions": [
        "poimandres.poimandres",
        "slevesque.shader",
        "GitHub.copilot"
      ]
    }
  },
  "postCreateCommand": "npm install"
}